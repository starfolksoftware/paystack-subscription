{
    "name": "starfolksoftware/paystack-subscription",
    "description": "Provides an inteface to paystack's subscription service",
    "keywords": [
        "starfolksoftware",
        "paystack-subscription"
    ],
    "homepage": "https://github.com/starfolksoftware/paystack-subscription",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/starfolksoftware/paystack-subscription/issues",
        "source": "https://github.com/starfolksoftware/paystack-subscription"
    },
    "authors": [
        {
            "name": "Faruk Nasir",
            "email": "faruk@starfolksoftware.com",
            "homepage": "https://starfolksoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/log": "^6.0|^7.0|^8.0",
        "illuminate/notifications": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/view": "^6.0|^7.0|^8.0",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/intl": "^4.3|^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Starfolksoftware\\PaystackSubscription\\": "src",
            "Starfolksoftware\\PaystackSubscription\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Starfolksoftware\\PaystackSubscription\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Starfolksoftware\\PaystackSubscription\\PaystackSubscriptionServiceProvider"
            ],
            "aliases": {
                "Subscription": "Starfolksoftware\\PaystackSubscription\\SubscriptionFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
